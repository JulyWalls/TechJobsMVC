<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@for (int i = 0; i < ViewBag.jobs.Count; i++)
{
    <table class="job-listing">
        <tbody>

            @foreach (KeyValuePair<string, string> job in ViewBag.jobs[0])
            {
                <tr>
                    <td>@job.Key</td>
                    <td>@job.Value</td>
                </tr>
            }
        </tbody>
    </table>

}
 }